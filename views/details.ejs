<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
</head>
<body>
    <h3><%= item.employeeID %></h3>
    <p><%= item._id %></p>
    <% item.document.forEach((res) =>{%>
            <div> 
                <a href="/doc/<%= res %>"><p><%= res%></p></a>
            </div>
            
    <%})%>
    
    <a id="delete" data-del="<%= item.employeeID%>">delete</a>

    <script>
        const endpoint= document.getElementById('delete')
        endpoint.addEventListener('click',(e)=>{
    const url = `/${endpoint.dataset.del}`
    fetch(url,{method: 'DELETE'})
    .then((response)=>{return response.json()})
    .then((response)=>{
        console.log(response)
        window.location.href = response.redirect
    })
    .catch((error)=>{
        console.log(error)
    })
})
    </script>
</body>
</html>